.container {
    overflow: hidden;
}

.container .title {
    background: #fff;
    font-size: 18px;
    font-weight: 700;
    padding: 26rpx 25rpx;
    text-align: center;
}

.top-radius {
    border-top-left-radius: 10rpx;
    border-top-right-radius: 10rpx;
}

.bottom-radius {
    border-bottom-left-radius: 10rpx;
    border-bottom-right-radius: 10rpx;
}
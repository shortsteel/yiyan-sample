[is="@vant/weapp/dist/goods-action-button/index"] {
    flex: 1;
}

[is="@vant/weapp/dist/icon/index"] {
    align-items: center;
    display: inline-flex;
    justify-content: center;
}

[is="@vant/weapp/dist/loading/index"] {
    font-size: 0;
    line-height: 1;
}

[is="@vant/weapp/dist/tab/index"] {
    flex-shrink: 0;
    width: 100%;
}

@media (prefers-color-scheme:dark) {
    page {
        --action-sheet-item-background: #191919;
        --action-sheet-cancel-padding-color: #111;
        --action-sheet-cancel-text-color: hsla(0,0%,100%,.8);
        --active-color: #242424;
    }

    button {
        color: hsla(0,0%,100%,.8);
    }
}

page {
    --tabs-nav-background-color: transparent;
    --field-input-text-color: hsla(0,0%,100%,.8);
    --cell-background-color: transparent;
    --divider-border-color: #242424;
    --button-default-background-color: transparent;
    --button-default-color: #fff;
    --image-loading-icon-color: hsla(0,0%,100%,.6);
    --image-placeholder-background-color: hsla(0,0%,100%,.06);
    background: #111;
    color: hsla(0,0%,100%,.7);
    font-family: PingFang SC,miui,system-ui,-apple-system,BlinkMacSystemFont,Helvetica Neue,Helvetica,sans-serif;
}

image,input,navigator,scroll-view,swiper,textarea,video,view {
    box-sizing: border-box;
}

button {
    border: 0;
    border-radius: 0;
    margin: 0;
    outline: 0;
    padding: 0;
}

button:after {
    border: 0;
    outline: 0;
}

.loading {
    display: flex;
    height: 24px;
    justify-content: center;
    position: fixed;
    top: 80rpx;
    width: 100%;
    z-index: 1;
}

